{
  "name": "acm-template-pro-generator",
  "description": "ACM 示例模板生成器",
  "basePackage": "com.peng",
  "version": "1.0",
  "author": "peng",
  "createTime": "2023-11-22",
  "fileConfig": {
    "inputRootPath": "/code/java/project/peng-generator/peng-genertor-demo-projects/acm-template-pro",
    "outputRootPath": "generated",
    "type": "dir",
    "files": [
      {
        "inputPath": "src/com/peng/acm/MainTemplate.java.flt",
        "outputPath": "src/com/peng/acm/MainTemplate.java",
        "type": "file",
        "generateType": "dynamic"
      },
      {
        "inputPath": ".gitignore",
        "outputPath": ".gitignore",
        "type": "file",
        "generateType": "static"
      },
      {
        "inputPath": "README.md",
        "outputPath": "README.md",
        "type": "file",
        "generateType": "static"
      }
    ]
  },
  "modelConfig": {
    "models": [
      {
        "fileName": "loop",
        "type": "boolean",
        "description": "是否生成循环",
        "defaultValue": false,
        "addr": "1"
      },
      {
        "fileName": "author",
        "type": "String",
        "description": "作者注释",
        "defaultValue": "peng",
        "addr": "a"
      },
      {
        "fileName": "outputText",
        "type": "String",
        "description": "输出信息",
        "defaultValue": "sum = ",
        "addr": "o"
      }
    ]
  }
}